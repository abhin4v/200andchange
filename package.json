{
  "private": true,
  "devDependencies": {
    "docco": "^0.9.1",
    "remark-cli": "^12.0.1",
    "remark-html": "^16.0.1"
  },
  "scripts": {
    "docco": "find . \\( -name '*.js' -o -name '*.py' \\) ! -path './node_modules/*' ! -path './scripts/*' | xargs docco",
    "gen-site": "npm run docco && node _scripts/generateIndex.mjs > docs/index.html"
  }
}
